algoritmo "Operadores_lógicos"
// Função : mostra como usar os operadores lógicos
// Autor : boson_Treinamento
// Data : 02/04/2013
//Aula_9 : Operadores e expressões lógicas
var
   v, f, res : logico

inicio
// Seção de Comandos
      v <- verdadeiro
      f <- falso
      res <- (nao nao f ou nao f) e (v ou f)
      escreval(res)
fimalgoritmo
